SELECT COUNT(*) FROM emp WHERE Sal>1000 ;
SELECT MIN(Sal) as minimum,MAX(Sal) as maximun,AVG(Sal) as Average FROM emp;
SELECT Loc,COUNT(EmpNo) FROM emp a right join dept b on a.deptNo= b.deptNo GROUP BY Loc ;
SELECT * FROM emp ORDER BY Ename DESC;
