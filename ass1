SELECT * FROM `emp` WHERE Ename REGEXP '^[a-k].*$';
SELECT a.Ename as emp FROM emp a,emp b WHERE  b.EmpNo=a.Mgr AND b.Ename= 'Stefen';
SELECT Ename from emp where EmpNo in (SELECT Mgr FROM emp GROUP BY Mgr  HAVING COUNT(*) IN (  SELECT MAX(number)    FROM (       SELECT Mgr, COUNT(*) AS number       FROM emp       GROUP BY Mgr    ) subquery));
